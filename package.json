{
  "name": "Mood",
  "version": "0.2.2",
  "description": "Kowhai 2018 - Final Project",
  "repository": "https://github.com/kowhai-2018/Final-Project.git",
  "main": "server/index.js",
  "author": "Kowhai 2018",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "server": "nodemon server",
    "start": "webpack && node server",
    "updateDB": "rm -f dev.sqlite3 && yarn knex migrate:latest && yarn knex seed:run",
    "postinstall": "webpack -p --progress --config ./webpack-config/webpack.prod.js --mode production && knex migrate:latest && yarn knex seed:run",
    "test": "jest --watchAll",
    "build-dev": "webpack --progress --config ./webpack-config/webpack.dev.js --mode development",
    "build-prod": "webpack -p --progress --config ./webpack-config/webpack.prod.js --mode production",
    "dev": "nodemon --watch server/index.js & yarn build-dev --watch",
    "prod": "nodemon --watch server/index.js & yarn build-prod --watch"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chart.js": "^2.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "heroku-ssl-redirect": "^0.0.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "libsodium-wrappers-sumo": "^0.7.3",
    "moment": "^2.22.2",
    "pg": "^7.7.1",
    "react": "^16.6.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.83.0",
    "superagent": "^4.0.0",
    "workbox-build": "^3.6.3",
    "workbox-cli": "^3.6.3",
    "workbox-webpack-plugin": "^3.6.3",
    "webpack": "^4.27.1",
    "webpack-merge": "^4.1.5"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "nock": "^10.0.2",
    "nodemon": "^1.18.7",
    "react-test-renderer": "^16.6.3",
    "sqlite3": "^4.0.4",
    "supertest": "^3.3.0",
    "webpack-cli": "^3.1.2"
  }
}
